[Unit]
Description=Gunicorn Web Server as Unit Service Systemd - TutLinks.com
After=network.target

[Service]
User=tmmuse_server
Group=www-data
WorkingDirectory=/home/tmmuse_server/TmMuse_api
Environment="PATH=/home/tmmuse_server/TmMuse_api/venv/bin"
ExecStart=/usr/local/bin/gunicorn --config /home/tmmuse_server/TmMuse_api/gunicorn.py main:app

[Install]
WantedBy=multi-user.target